class Solution {
    public int searchInsert(int[] nums, int target) {
        int i = 0;
        while(i < nums.length && nums[i] < target) 
        { 
            i++;
        }
    return i;
    }
}


// nums[i] < target makes it easy as when the target is reaced then it will return the array and if the nums[i] gets bigger than it , it returns the position where to enter
